<div  class="form">
    
<%= form_for(@event) do |f| %>
  <% if @event.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@event.errors.count, "error") %> prohibited this event from being saved:</h2>

      <ul>
      <% @event.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
    
      <div class="field-wrap">
        <%= f.label :title %><br>
        <%= f.text_field :title, placeholder: "Title of event"%>
      </div>
      <div class="field-wrap">
        <%= f.label :venue %><br>
        <%= f.text_field :venue %>
      </div>
      <div class="field-wrap">
        <%= f.label :starts_at, value: "start time" %><br>
        <%= f.datetime_select :starts_at %>
      </div>
      <div class="field-wrap">
        <%= f.label :pic %><br>
        <%= f.text_field :pic %>
      </div>
      <div class="actions">
        <%= f.submit %>
      </div>
<% end %>
</div>